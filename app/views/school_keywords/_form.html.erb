<%= form.fields_for :school_keywords do |key_form| %>
    <div class="field">
      <%= key_form.label :name, '关键字:' %>
      <%= key_form.text_field :name %>
    </div>
    <% unless key_form.object.nil? || key_form.object.new_record? %>
        <div class="field">
          <%= key_form.label :_destroy, 'Remove:' %>
          <%= key_form.check_box :_destroy %>
        </div>
    <% end %>
<% end %>